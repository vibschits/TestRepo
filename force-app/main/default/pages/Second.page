<apex:page controller="AccountAll">

    <apex:pageBlock title="My Content">

        <apex:pageBlockTable value="{!accountList}" var="item">

            <apex:column value="{!item.name}"/>
            <apex:column value="{!item.Rating}"/>
            <apex:column value="{!item.Industry}"/>
            

        </apex:pageBlockTable>

    </apex:pageBlock>

</apex:page>